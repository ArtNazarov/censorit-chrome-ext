{
  "name": "CensorIt",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Block negative content. Nazarov A.A., Orenburg, 2024",
  "homepage_url": "http://github.com/artnazarov",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
             
    ],
    "persistent": true
  },
  "options_page": "/src/dialog.html",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "tts",
    "ttsEngine",
    "history",
    "idle",
    "management",
    "notifications",
    "tabs",
    "storage",
    "geolocation",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        
        "src/inject/inject.js" 
		
		
      ]
    }
  ]
}